var aos = ["mom", "js", "malayalam", "html", "css", "dad"];
var op = [];
var i = 0;

while (i < aos.length) {
    var word = aos[i];
    var new = word.split('').reverse().join('');
    if (word === new) {
        op.push(word);
    }
    i++;
}

console.log(op);  // Output: ["mom", "malayalam", "dad"]
